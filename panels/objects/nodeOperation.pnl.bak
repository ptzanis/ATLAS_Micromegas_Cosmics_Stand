   RootPanelOnModule( \"objects/extraHolder.pnl\",\"extraHolder\", \"extraModule\"+\"_N\"+node, makeDynString(\"$node:\"+node), 0, 0, 0, 1, 1); 
   
  
     }    
     
     
     
     
     
     
    if(mode==\"CosmicsBasic\" || mode==\"CosmicsExtra\")     
     {  

       
