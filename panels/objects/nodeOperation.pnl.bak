        dpGet(\"N\"+i+\".Mode\",mode);
        dpGet(\"N\"+i+\".ChamberType\",type);
        dpGet(\"N\"+i+\".BatchId\",batchID);
        
        addSymbol(myModuleName(), myPanelName(), \"objects/nodeActionCosmics.pnl\", \"nodeAction\"+node+\"_\"+type,
