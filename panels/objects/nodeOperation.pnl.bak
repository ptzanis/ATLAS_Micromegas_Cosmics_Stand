"main(mapping event)
{
  string mode=$mode;
  string node=$node;
  string sector=$sector;
  int startNodeCosmics;
  if(mode==\"ValidationQA\")
  {
     time startTime=getCurrentTime();
     dpSet(\"N\"+node+\".Archiving.StartTime\",startTime); 
     testStatus.backCol(\"S7_stateWentUnq\");
     startField.text(hour(startTime)+\":\"+minute(startTime)+\" \"+day(startTime)+\"/\"+month(startTime)+\"/\"+year(startTime));
     this.enabled(FALSE);
     stopButton.enabled(TRUE);
  }
  
  if(mode==\"CosmicsBasic\" || mode==\"CosmicsExtra\" )
  {
     if(sector==\"1\")
       startNodeCosmics=1;
     if(sector==\"2\")
       startNodeCosmics=5;     
     
     time startTime=getCurrentTime();
     testStatus.backCol(\"S7_stateWentUnq\");
     startField.text(hour(startTime)+\":\"+minute(startTime)+\" \"+day(startTime)+\"/\"+month(startTime)+\"/\"+year(startTime));
     this.enabled(FALSE);
     stopButton.enabled(TRUE);     
     for(int i=startNodeCosmics;i<startNodeCosmics+4;i++)
     {
     dpSet(\"N\"+i+\".Archiving.StartTime\",startTime);                   
     }   
  }  
  
  

    
