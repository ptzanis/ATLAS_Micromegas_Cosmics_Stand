   delay(1);
   RootPanelOnModule( \"objects/extraHolder.pnl\",\"extraHolder\", \"extraModule\"+\"_N\"+sector, makeDynString(\"$node:\"+sector), 0, 0, 0, 1, 1);      
     
     
    }

