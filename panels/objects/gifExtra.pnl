        fwArchive_configMultiple(archivedDpes[i] + \".EnvParameters.Atmospheric_Pressure\", archiveClassName, archiveType, smoothProcedure,
                          deadband, timeInterval, exceptionInfo, checkClass);
        if (dynlen(exceptionInfo)>0) DebugN(exceptionInfo);
    
        fwArchive_configMultiple(archivedDpes[i] + \".EnvParameters.Humidity_Inside_Bunker\", archiveClassName, archiveType, smoothProcedure,
