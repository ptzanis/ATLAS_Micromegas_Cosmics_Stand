               fwArchive_configMultiple(archivedDpes[i] + \".Attenuators.UpStreamPos.A\", archiveClassName, archiveType, smoothProcedure,
                          deadband, timeInterval, exceptionInfo, checkClass);
        if (dynlen(exceptionInfo)>0) DebugN(exceptionInfo);
                fwArchive_configMultiple(archivedDpes[i] + \".Attenuators.UpStreamPos.B\", archiveClassName, archiveType, smoothProcedure,
                          deadband, timeInterval, exceptionInfo, checkClass);
        if (dynlen(exceptionInfo)>0) DebugN(exceptionInfo);
                fwArchive_configMultiple(archivedDpes[i] + \".Attenuators.UpStreamPos.C\", archiveClassName, archiveType, smoothProcedure,
                          deadband, timeInterval, exceptionInfo, checkClass);
        if (dynlen(exceptionInfo)>0) DebugN(exceptionInfo);
                fwArchive_configMultiple(archivedDpes[i] + \".Attenuators.UpStreamPos.EffectiveAttenuation\", archiveClassName, archiveType, smoothProcedure,
