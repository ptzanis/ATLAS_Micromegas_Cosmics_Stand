        if (dynlen(exceptionInfo)>0) DebugN(exceptionInfo);
                fwArchive_configMultiple(archivedDpes[i] + \".EnvParameters.Temp_Outside_Bunker\", archiveClassName, archiveType, smoothProcedure,
