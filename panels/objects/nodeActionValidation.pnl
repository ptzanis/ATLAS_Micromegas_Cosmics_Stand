  dpGet(\"N\"+node+\".Mapping.ChannelsTotal.Drift\",driftChannels);
  
  dynAppend(channelSetOn,stripChannels);
  dynAppend(channelSetOn,driftChannels);
  
//   DebugN(channelSetOn);
  
  for(int i=1;i<=dynlen(channelSetOn);i++)
  {
    dpGet(\"N\"+node+\".Mapping.\"+channelSetOn[i]+\".Channel\",channel);
     dpSetWait(\"CAEN/\"+channel+\".settings.onOff:_original.._value\", 1); 
//   dpSetWait(\"CAEN/\"+channel+\".actual.status:_original.._value\", 1);       
  }
  
//   DebugN(\"Sector: \"+sector+\", Node: \"+node+\" ON \"+\"!\");
  touchChannels();
  
  
