//  DebugN(node[i+1],mode[i+1],chamber[i+1],batchID[i+1],env[i+1],gas[i+1],hum[i+1],gif[i+1],startTime[i+1],endTime[i+1]);

 
 
 if(batchID[i+1]==batch) 
