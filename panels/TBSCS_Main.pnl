void EP_setBackColor(string node,string layer)
{
  dyn_errClass err;

  if( !dpExists( \"System1:N\"+node+\".Mapping.\"+layer+\"S\"+\".Channel:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  dpConnect(\"EP_setBackColorCB\", 
            \"System1:N\"+node+\".Mapping.\"+layer+\"S\"+\".Channel:_online.._value\");
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");

}


void EP_setBackColorCB(string dpSource, string sNewValue)
{
  if (sNewValue == \"\"\"\")
    setValue(\"\", \"backCol\", \"Red\");
  else
    setValue(\"\", \"backCol\", \"Green\");
}

