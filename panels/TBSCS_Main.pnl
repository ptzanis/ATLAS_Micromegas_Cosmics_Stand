"main()
{
    this.enabled(FALSE);
    this.text(\"Please wait...\");
    
    string sparkPath = PROJ_PATH + \"scripts/sparkGNU.bat\";
    string gnuPath = PROJ_PATH + \"gnuplot/bin\";
    string plotFormatPath = PROJ_PATH + \"scripts/plotFormatGNU.bat\";
    string alias;
    string tempPath = PROJ_PATH + \"temp/\";
    string currentDate = formatTime(\"%d_%m\", getCurrentTime());
    
    int err = editCreateSpark(sparkPath, plotFormatPath, gnuPath);
    //DebugN(\"Error value returned:\" + err);
    
    if (!exportTempData()) DebugN(\"Couldn't delete temp folder. Please delete old files manually.\",
                                  \"Path: \" + PROJ_PATH + \"temp\");
    
    dyn_dyn_string channelDatPath; // channelDatPath[1][i] Channel Names [2][i] .dat Paths
    
    dyn_string allChannels;
    allChannels = dpNames(\"*\", \"FwCaenChannel\");
    
    for (int i = 1; i <= dynlen(allChannels); i++) {
        alias = dpGetAlias(allChannels[i] + \".\");
        
        if (alias == \"\") {
            channelDatPath[1][i] = substr(allChannels[i], strlen(getSystemName()) + 5);
            strreplace(channelDatPath[1][i], \"/\", \" \");
        }
        else
            channelDatPath[1][i] = alias;
        
        allChannels[i] = substr(allChannels[i], strpos(allChannels[i], \"CAEN\") + 5);
        strreplace(allChannels[i], \"/\", \"_\");
        channelDatPath[2][i] = tempPath + \"vMon_\" + allChannels[i] + \"_\" + currentDate + \".dat\";
        channelDatPath[3][i] = tempPath + \"iMon_\" + allChannels[i] + \"_\" + currentDate + \".dat\";
    }
    
   // DebugN(channelDatPath);

    for (int i = 1; i <= dynlen(channelDatPath[1]); i++) {
        createPlotFormatBatch(plotFormatPath, makeDynString(channelDatPath[1][i], channelDatPath[2][i], channelDatPath[3][i]));
        runBatch(sparkPath);
    }
    
    // cleanTemp();
    this.text(\"Export Plots\");
    this.enabled(TRUE);
