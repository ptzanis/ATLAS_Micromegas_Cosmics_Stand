    textField2.text = \"\";
    textField3.text = \"\";
    textField4.text = \"\";
    textField5.text = \"\";
    textField6.text = \"\";
    textField7.text = \"\";
    
  //  delay(3);
    channelsTable.deleteAllLines;
    refreshConfigurationTable();  
}
